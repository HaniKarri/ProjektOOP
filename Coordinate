using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SHAPESOOP
{
    public class Coordinate
    {
        
        public int x { get; private set; }
        public int y { get; private set; }

        public Coordinate(int posX, int posY)
        {
            x = posX;
            y = posY;
        }

    }
}

